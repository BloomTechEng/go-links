{
  "name": "Trotto go links",
  "keywords": [
    "productivity"
  ],
  "website": "https://www.trot.to",
  "repository": "https://github.com/trotto/go-links",
  "logo": "https://www.trot.to/img/logos/trotto.svg",
  "env": {
    "FLASK_SECRET": {
      "description": "A secret used to sign session cookies and CSRF tokens. See https://www.trot.to/docs/deploy/app-yml-config-file"
    },
    "TROTTO_CONFIG": {
      "description": "A base64-encoded app.yml file. See https://www.trot.to/docs/deploy/app-yml-config-file"
    },
    "GOOGLE_OAUTH_CLIENT_JSON": {
      "description": "The JSON for a Google OAuth client"
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "hobby"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql:hobby-dev",
      "options": {
        "version": "12"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ]
}
